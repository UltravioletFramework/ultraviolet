<?xml version="1.0" encoding="utf-8" ?>
<LocalizedStrings>
  <String Key="CANNOT_CREATE_HEADLESS_CONTEXT_ON_MOBILE">
    <en-US>
      <Variant>A headless context cannot be created on the Android or iOS platforms.</Variant>
    </en-US>
  </String>
  <String Key="BUFFER_IS_TOO_SMALL">
    <en-US>
      <Variant>Buffer '{0}' is too small for the requested data.</Variant>
    </en-US>
  </String>
  <String Key="SURFACE_ALREADY_PREPARED_FOR_EXPORT">
    <en-US>
      <Variant>The surface has already been prepared for export.</Variant>
    </en-US>
  </String>
  <String Key="SURFACE_MUST_HAVE_SQUARE_LAYERS">
    <en-US>
      <Variant>A Surface3D which is loaded from a single file must consist of square layers arranged from left to right, and the file must contain no excess space.</Variant>
    </en-US>
  </String>
  <String Key="MISSING_GRAPHICS_ASSEMBLY">
    <en-US>
      <Variant>No graphics subsystem assembly was specified.</Variant>
    </en-US>
  </String>
  <String Key="INVALID_GRAPHICS_ASSEMBLY">
    <en-US>
      <Variant>Invalid graphics subsystem assembly.</Variant>
    </en-US>
  </String>
  <String Key="MISSING_AUDIO_ASSEMBLY">
    <en-US>
      <Variant>No audio subsystem assembly was specified.</Variant>
    </en-US>
  </String>
  <String Key="INVALID_AUDIO_ASSEMBLY">
    <en-US>
      <Variant>Invalid audio subsystem assembly.</Variant>
    </en-US>
  </String>
  <String Key="MISSING_GRAPHICS_FACTORY">
    <en-US>
      <Variant>Missing graphics factory.</Variant>
    </en-US>
  </String>
  <String Key="MISSING_AUDIO_FACTORY">
    <en-US>
      <Variant>Missing audio factory.</Variant>
    </en-US>
  </String>
  <String Key="PRIMARY_WINDOW_ALREADY_INITIALIZED">
    <en-US>
      <Variant>The application's primary window has already been initialized.</Variant>
    </en-US>
  </String>
  <String Key="PRIMARY_WINDOW_MUST_BE_INITIALIZED">
    <en-US>
      <Variant>The application's primary window must be initialized by the Graphics subsystem before its constructor returns.</Variant>
    </en-US>
  </String>
</LocalizedStrings>